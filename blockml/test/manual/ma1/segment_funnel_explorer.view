- measure: count_sessions_event12
  type: custom
  sql: COUNT(DISTINCT CASE WHEN (
    
    (${event1_ts_frames___time} IS NOT NULL) AND
    (${event2_ts_frames___time} IS NOT NULL) AND
    (${event1_before_event2} = 'Yes')
    
    ) THEN ${session_id} ELSE NULL END)  

