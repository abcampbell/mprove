
# track_facts

model: segment_m2
group: 'segment'

joins:
- from_view: segment_4_track_facts
  as: a

- join_view: segment_tracks
  as: b
  sql_on: ${a.timestamp} = ${b.timestamp} and ${a.anonymous_id} = ${b.anonymous_id}
  
- join_view: segment_3_session_tracks
  as: c
  sql_on: ${a.session_id} = ${c.session_id}
  
- join_view: segment_5_session_track_facts
  as: d
  sql_on: ${a.session_id} = ${d.session_id}  
  
- join_view: segment_6_user_session_facts
  as: e
  sql_on: ${a.mprove_visitor_id} = ${e.mprove_visitor_id}
  
- join_view: segment_tracks_flow
  as: f
  sql_on: ${a.event_id} = ${f.event_id}  

  
