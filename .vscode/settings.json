{
  "prettier.disableLanguages": ["vue", "html"],
  "html.format.wrapAttributes": "force-aligned",
  "tslint.ignoreDefinitionFiles": false,
  "search.exclude": {
    "**/node_modules": true,
    "**/dist": true,
  },  
  "files.associations": {
    "*.dashboard": "yaml",
    "*.model": "yaml",
    "*.view": "yaml",
    "*.udf": "yaml"
  },  
  "cSpell.words": [
    "id",
    "mconfig",
    "mconfigs",
    "ngrx",
    "repo",
    "struct",
    "structs",
    "backend",
    "blockml",
    "colorpicker",
    "injectable",
    "pdts",
    "preloading",
    "repos",
    "sortings",
    "timezones",
    "validators",
    "webhook",
    "whitelisted",
    "yesno",
    "bahmutov",
    "codelyzer",
    "compodoc",
    "devkit",
    "devtools",
    "fontawesome",
    "fortawesome",
    "hammerjs",
    "jasminewd",
    "jcrop",
    "mprove",
    "ngtools",
    "polyfill",
    "polyfills",
    "preaccessibility",
    "precy",
    "swimlane",
    "uglifyjs",
    "webchat"
  ]
