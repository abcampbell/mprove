view: segment_users
derived_table: |
  SELECT2
    * EXCEPT (ROW_NUMBER)
  FROM (
    SELECT
      *,
      ROW_NUMBER() OVER (PARTITION BY id ORDER BY received_at DESC) ROW_NUMBER
    FROM
      `mprove-demo.segment.users`)
  WHERE
    ROW_NUMBER = 1
permanent: true  
pdt_trigger_time: '0 * * * * *'  

fields:
  
# - dimension: received_at
#   sql: received_at

- dimension: name
  sql: name
  
- dimension: user_id
  sql: id
 
- dimension: email
  sql: email
 
# - dimension: anonymous_id
#   sql: anonymous_id

- measure: count_users
  type: count_distinct
  sql: ${user_id}

