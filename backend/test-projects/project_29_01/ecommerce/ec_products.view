view: ec_products
label: 'Products'
table: mprove-demo.ecommerce.products

fields:
- dimension: product_id
  sql: product_id
  
- dimension: retail_price
  sql: retail_price
  
- dimension: cost
  sql: cost
  
- dimension: category
  sql: category
  
- dimension: brand
  sql: brand
  
- measure: count
  type: count_distinct
  sql: ${product_id}   