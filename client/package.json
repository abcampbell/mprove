    "@types/js-yaml": "^3.5.30",
    "@types/node": "^10.12.2",
    "@types/uuid": "^2.0.29",
    "codelyzer": "~4.5.0",
    "express": "^4.16.4",
    "madge": "3.3.0",
    "ngrx-store-freeze": "0.2.4",
    "npm-run-all": "^4.1.5",
